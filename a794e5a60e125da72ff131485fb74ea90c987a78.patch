+--
+-- A semigroupoid satisfies all of the requirements to be a Category except
+-- for the existence of identity arrows.
+----------------------------------------------------------------------------
+module Data.Semigroupoid
+  ( Semigroupoid(..)
+  , WrappedCategory(..)
+  , Semi(..)
+  ) where
+
+import Control.Arrow
+import Data.Functor.Bind
